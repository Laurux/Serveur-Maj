Public Function convBStr(bln As String) As String

  Dim sRet As String

  If bln = "0" Or If bln = "" Then
    sRet = "0"
  Else
    sRet = "1"
  Endif
  Return sRet

End

Public Sub MajEqu(code As String, cequ As String)

